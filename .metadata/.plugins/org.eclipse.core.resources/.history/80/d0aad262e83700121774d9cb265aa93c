/**
 * Define behavior of all UI components
 * @author Gui
 *
 */
public class UIBehavior implements OnClickListener {

	public void onClick(View v) {
		
		if(v.getId() == R.id.checkBoxData)
		{
			CheckBox cb = (CheckBox)v;
		}
		
	}

}
